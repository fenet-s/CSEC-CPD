class Solution:
    def findContentChildren(self, g: List[int], s: List[int]) -> int:
        g.sort()
        s.sort()
        child = cookie=0
        ans=0
        while child < len(g) and cookie < len(s):
            if s[cookie] >= g[child] :
                ans +=1
                child+=1
            cookie +=1
        
        return ans
        
